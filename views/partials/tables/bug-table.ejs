<!-- Table -->
<table id="myTable" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <th>Title</th>
        <th>Description</th>
        <th>Reported</th>
        <th>Assignee</th>
        <th>Resolved</th>
        <th>Days Left</th>
    </thead>
    <tbody>
        <% bugs.forEach((bug) => { %>
        
        <tr <% if(bug.daysLeft <= 0 && !bug.resolved) { %> style="background-color: lightyellow" <% } %> 
            <% if(bug.resolved) { %> style="background-color: #dcffe4" <% } %> >
            <td><%= bug.title %></td>
            <td><%= bug.description %></td>
            <td><%= bug.reported %></td>
            <td><%= bug.assignee %></td>
            <td><%= bug.resolved ? "Yes" : "No" %></td>
            <td><%= bug.daysLeft %> </td>
        </tr>
        
        <% }); %> 
    </tbody>
</table>